<?xml version="1.0"?>
<!DOCTYPE hibernate-mapping PUBLIC
        "-//Hibernate/Hibernate Mapping DTD 3.0//EN"
        "http://hibernate.sourceforge.net/hibernate-mapping-3.0.dtd">
 
<hibernate-mapping>
    <class name="com.theavocadopapers.apps.kqool.entity.ClientToBackendUserMapping" table="client_to_backend_user_mapping">
        <cache usage="read-write" />
        
        <id name="id" column="id">
            <generator class="native"/>
        </id>

			<!-- Used by the framework... -->
			<property name="lastModifiedDate" column="last_modified_date" />
			<property name="createDate" column="create_date" />
			<!-- ...end used by the framework. -->
			
			<property name="clientUserId" column="client_user_id" />
			<property name="backendUserId" column="backend_user_id" />

    </class>
</hibernate-mapping>
