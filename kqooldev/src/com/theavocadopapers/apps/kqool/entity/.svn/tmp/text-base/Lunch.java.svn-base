package com.theavocadopapers.apps.kqool.entity;

import java.util.Date;
import java.util.List;

import com.theavocadopapers.core.logging.Logger;

public class Lunch extends AbstractDbObject{

	private static final Class currentClass=Lunch.class;
	
	private static final Logger logger = Logger.getLogger(currentClass);
	
	int lunchId;
	String data;
	int adminUserId;
	
	@Override
	protected Comparable getComparableValue() {
		return new Integer(getId());
	}

	public int getLunchId() {
		return lunchId;
	}

	public void setLunchId(int lunchId) {
		this.lunchId = lunchId;
	}

	public String getData() {
		return data;
	}

	public void setData(String data) {
		this.data = data;
	}

	public int getAdminUserId() {
		return adminUserId;
	}

	public void setAdminUserId(int adminUserId) {
		this.adminUserId = adminUserId;
	}
	
	public static Lunch getById(final int id) {
		return (Lunch)getById(currentClass, id, false);
	}
	
	public static List getAll() {
		return getAll(currentClass);
	}
	
	public static Lunch getByUserId(final int userId) {
		return (Lunch)getUniqueByField("adminUserId", ""+userId,FIELD_TYPE_NUMERIC, currentClass, false);
	}

}
