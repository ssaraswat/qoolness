<?xml version="1.0"?>
<!DOCTYPE hibernate-mapping PUBLIC
        "-//Hibernate/Hibernate Mapping DTD 3.0//EN"
        "http://hibernate.sourceforge.net/hibernate-mapping-3.0.dtd">
 
<hibernate-mapping>
    <class name="com.theavocadopapers.apps.kqool.entity.Dinner" table="Dinner">
        <cache usage="read-write" />
        
        <id name="dinnerId" column="dinnerId">
            <generator class="native"/>
        </id>
        
        <!-- Used by the framework... -->
			<property name="lastModifiedDate" column="last_modified_date" />
			<property name="createDate" column="create_date" />
			<!-- ...end used by the framework. -->
			
			
			<property name="data" column="data" type="string"/>
			<property name="adminUserId" column="adminUserId" type="integer"/>
    </class>
</hibernate-mapping>