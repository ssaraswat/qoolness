<?xml version="1.0"?>
<!DOCTYPE hibernate-mapping PUBLIC
        "-//Hibernate/Hibernate Mapping DTD 3.0//EN"
        "http://hibernate.sourceforge.net/hibernate-mapping-3.0.dtd">
 
<hibernate-mapping>
    <class name="com.theavocadopapers.apps.kqool.entity.PreRegistrationUserData" table="pre_registration_user_data">
  	  	<cache usage="read-write" />
        <id name="id" column="id">
            <generator class="native"/>
        </id>

			<!-- Used by the framework... -->
			<property name="lastModifiedDate" column="last_modified_date" />
			<property name="createDate" column="create_date" />
			<!-- ...end used by the framework. -->
			
			<property name="lastName" column="last_name" />
			<property name="firstName" column="first_name" />
			<property name="username" column="username" />
			<property name="emailAddress" column="email_address" />
			<property name="comments" column="comments" type="text" />
			<property name="gender" column="gender" />
			<property name="siteId" column="site_id" />
    </class>
</hibernate-mapping>
