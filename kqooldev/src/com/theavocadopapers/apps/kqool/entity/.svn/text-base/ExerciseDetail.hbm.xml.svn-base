<?xml version="1.0"?>
<!DOCTYPE hibernate-mapping PUBLIC
        "-//Hibernate/Hibernate Mapping DTD 3.0//EN"
        "http://hibernate.sourceforge.net/hibernate-mapping-3.0.dtd">
 
<hibernate-mapping>
    <class name="com.theavocadopapers.apps.kqool.entity.ExerciseDetail" table="exercise_detail">
        <id name="id" column="id">
            <generator class="native"/>
        </id>

			<!-- Used by the framework... -->
			<property name="lastModifiedDate" column="last_modified_date" />
			<property name="createDate" column="create_date" />
			<!-- ...end used by the framework. -->
			
			<property name="exerciseId" column="exercise_id" />
			<property name="workoutId" column="workout_id" />
			<property name="quantity" column="quantity" />
			<property name="intensity" column="intensity" />
			<property name="reps" column="reps" />
			<property name="index" column="ed_idx" />
			<property name="supersetGroup" column="superset_group" />
			<property name="comments" column="comments" />
			<property name="restInterval" column="rest_interval" />
			<property name="calorieHourId" column="calorie_hour_id" />
			<property name="currentUserWeight" column="current_user_weight" />
			
			

    </class>
</hibernate-mapping>
