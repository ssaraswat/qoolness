<?xml version="1.0"?>
<!DOCTYPE hibernate-mapping PUBLIC
        "-//Hibernate/Hibernate Mapping DTD 3.0//EN"
        "http://hibernate.sourceforge.net/hibernate-mapping-3.0.dtd">
 
<hibernate-mapping>
    <class name="com.theavocadopapers.apps.kqool.entity.ClientToMeal" table="clientToMeal">
        <cache usage="read-write" />
        
        <id name="id" column="id">
            <generator class="native"/>
        </id>
        
        	<!-- Used by the framework... -->
			<property name="lastModifiedDate" column="last_modified_date" />
			<property name="createDate" column="create_date" />
			<!-- ...end used by the framework. -->
        
			<property name="mealType" column="mealType" type="string"/>
			<property name="clientId" column="clientId" type="integer"/>
			<property name="mealId" column="mealId" type="integer"/>
			<property name="date" column="date" type="timestamp"/>
			<property name="mealOrder" column="mealOrder" type="integer"/>
    </class>
</hibernate-mapping>