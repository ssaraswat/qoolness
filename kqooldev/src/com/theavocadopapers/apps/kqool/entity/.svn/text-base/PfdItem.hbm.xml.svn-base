<?xml version="1.0"?>
<!DOCTYPE hibernate-mapping PUBLIC
        "-//Hibernate/Hibernate Mapping DTD 3.0//EN"
        "http://hibernate.sourceforge.net/hibernate-mapping-3.0.dtd">
 
<hibernate-mapping>
    <class name="com.theavocadopapers.apps.kqool.entity.PfdItem" table="pfd_item">
        <id name="id" column="id">
            <generator class="native"/>
        </id>

			<!-- Used by the framework... -->
			<property name="lastModifiedDate" column="last_modified_date" />
			<property name="createDate" column="create_date" />
			<!-- ...end used by the framework. -->
			
			<property name="code" column="code" />
			<property name="value" column="value" />
			<property name="userId" column="user_id" />
			<property name="current" column="current" type="boolean" />
			<property name="settingUserId" column="setting_user_id" />
			<property name="subsequentValue" column="subsequent_value" />


	
	
    </class>
</hibernate-mapping>
