<?xml version="1.0"?>
<!DOCTYPE hibernate-mapping PUBLIC
        "-//Hibernate/Hibernate Mapping DTD 3.0//EN"
        "http://hibernate.sourceforge.net/hibernate-mapping-3.0.dtd">
 
<hibernate-mapping>
    <class name="com.theavocadopapers.apps.kqool.entity.CalorieDay" table="calorie_day">
        <id name="id" column="id">
            <generator class="native"/>
        </id>

			<!-- Used by the framework... -->
			<property name="lastModifiedDate" column="last_modified_date" />
			<property name="createDate" column="create_date" />
			<!-- ...end used by the framework. -->
			
			<property name="dateYear" column="date_year" />
			<property name="dateMonth" column="date_month" />
			<property name="dateDate" column="date_date" />
			<property name="userId" column="user_id" />
			<property name="weight" column="weight" type="float" />
			<property name="waist" column="waist" type="float" />
			<property name="chest" column="chest" type="float" />
			<property name="thigh" column="thigh" type="float" />
			<property name="hip" column="hip" type="float" />
			<property name="weekId" column="week_id" />
    </class>
</hibernate-mapping>
